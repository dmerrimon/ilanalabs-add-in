<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" 
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
           xsi:type="TaskPaneApp">

  <Id>f4e8c2a1-7b9d-4a3e-8f2c-1a5b7c9e0d3f</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Ilana Labs</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  
  <DisplayName DefaultValue="Ilana" />
  <Description DefaultValue="AI-powered clinical protocol assistant with Grammarly-style interface for real-time compliance checking." />

  <IconUrl DefaultValue="https://YOUR_AZURE_STATIC_APP_URL/icon-32.png" />
  <HighResolutionIconUrl DefaultValue="https://YOUR_AZURE_STATIC_APP_URL/icon-80.png" />

  <SupportUrl DefaultValue="https://github.com/YOUR_USERNAME/ilana-frontend" />
  
  <AppDomains>
    <AppDomain>https://YOUR_BACKEND_URL.onrender.com</AppDomain>
  </AppDomains>
  
  <Hosts>
    <Host Name="Document" />
  </Hosts>

  <Requirements>
    <Sets>
      <Set Name="WordApi" MinVersion="1.3" />
    </Sets>
  </Requirements>

  <DefaultSettings>
    <SourceLocation DefaultValue="https://YOUR_AZURE_STATIC_APP_URL" />
  </DefaultSettings>

  <Permissions>ReadWriteDocument</Permissions>

</OfficeApp>